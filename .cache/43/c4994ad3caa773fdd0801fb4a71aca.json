olid #666;\r\n  border-radius: 12px;\r\n  color: #666;\r\n  cursor: pointer;\r\n  transition: all 0.2s linear;\r\n}\r\n\r\n.card button:hover {\r\n  background-color: #fef9d7;\r\n  transform: translateY(-2px);\r\n  box-shadow: 2px 4px 10px rgba(0, 0, 0, 0.15);\r\n}\r\n\r\n.card button:active,\r\n.card button:focus {\r\n  outline: 0;\r\n}\r\n\r\n.todo {\r\n  grid-area: todo;\r\n  background-color: burlywood;\r\n}\r\n.doing {\r\n  grid-area: doing;\r\n  background-color: cadetblue;\r\n}\r\n.done {\r\n  grid-area: done;\r\n  background-color: chocolate;\r\n}\r\n\r\n.container {\r\n  display: grid;\r\n  grid-template-columns: 30% auto 30%;\r\n  grid-template-rows: auto;\r\n  grid-template-areas: \"todo doing done\";\r\n  border: 3px solid red;\r\n}\r\n\r\n.board {\r\n  border: 5px solid blueviolet;\r\n  .label {\r\n    text-align: center;\r\n    font-size: 1.7em;\r\n    font-weight: 600;\r\n  }\r\n}\r\n\r\n.task-list {\r\n  .task-card {\r\n    display: block;\r\n    width: 90%;\r\n    margin: 5%;\r\n    padding-left: 0.5em;\r\n    background-color: silver;\r\n\r\n    .task-name {\r\n      text-align: left;\r\n      font-size: 1em;\r\n      width: 90%;\r\n      margin: 5%;\r\n      padding-left: 0.5em;\r\n      background-color: white;\r\n      line-height: 2.7em;\r\n      border-radius: 5px;\r\n    }\r\n  }\r\n}\r\n\r\n@media (max-width: 700px) {\r\n  .container {\r\n    grid-template-areas:\r\n      \"doing\"\r\n      \"todo\"\r\n      \"done\";\r\n    grid-template-columns: 100%;\r\n    grid-template-rows: auto;\r\n  }\r\n\r\n  .board-doing {\r\n    .current-task-card {\r\n      display: block;\r\n      width: 90%;\r\n      margin: 5%;\r\n      padding-left: 0.5em;\r\n      background-color: rgb(41, 41, 41);\r\n    }\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a16e4636925e3ea49e87c733bc0b8de2","cacheData":{"env":{}}}